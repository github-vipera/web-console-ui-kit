{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "web-console-ui-kit": [
        "dist/web-console-ui-kit"
      ],
      "web-console-ui-kit/*": [
        "dist/web-console-ui-kit/*"
      ],
      "uikit-kitchensink-module": [
        "dist/uikit-kitchensink-module"
      ],
      "uikit-kitchensink-module/*": [
        "dist/uikit-kitchensink-module/*"
      ]
    }
  }
}
